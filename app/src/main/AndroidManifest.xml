<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.changui.android_mvvm_cleanarch_kotlin_flow">

    <application
        android:name=".StackOverFlowApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Androidmvvmcleanarchkotlinflow">
        <activity
            android:name="com.changui.android_mvvm_cleanarch_kotlin_flow.presentation.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Androidmvvmcleanarchkotlinflow.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>